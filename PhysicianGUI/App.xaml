                <ResourceDictionary Source="ResourceDictionaries/BorderDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/DataGridDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/TextFieldDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/Calendar.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/FlatComboBoxStyle.xaml"/>
